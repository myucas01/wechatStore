page {
  height: 100%;
  width: 100%;
  background: #f4f4f4;
}

.order-info {
  background: #fff;
  height: auto;
  overflow: hidden;
}

.item-a {
  line-height: 30rpx;
  font-size: 28rpx;
  color: #666;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right: 30rpx;
  border-bottom: 1px solid #f4f4f4;
  height: 70rpx;
}

.item-b {
  height: 29rpx;
  line-height: 29rpx;
  margin-top: 12.5rpx;
  margin-bottom: 41.5rpx;
  font-size: 30rpx;
  color: #666;
}

.item-c {
  border-top: 1px solid #f4f4f4;
  height: 103rpx;
  line-height: 103rpx;
}

.item-c .l {
  float: left;
}

.item-c .r {
  height: 103rpx;
  float: right;
  display: flex;
  align-items: center;
  padding-right: 16rpx;
}

.item-c .r .btn {
  float: right;
}

.item-c .cost {
  color: #b4282d;
}

.item-c .btn {
  line-height: 66rpx;
  border-radius: 5rpx;
  text-align: center;
  margin: 0 15rpx;
  padding: 0 20rpx;
  height: 66rpx;
}

.item-c .btn.active {
  background: #E71F19;
  color: #fff;
  font-size: 28rpx;
}

.order-goods {
  margin-top: 4rpx;
  /* background: #fff; */
}

.order-goods .h {
  height: 93.75rpx;
  line-height: 93.75rpx;
  border-bottom: 1px solid #f4f4f4;
  padding: 0 30rpx;
}

.order-goods .h .label {
  float: left;
  font-size: 28rpx;
  color: #333;
}

.order-goods .h .status {
  float: right;
  font-size: 28rpx;
  color: #E71F19;
}

.order-goods .item {
  height: auto;
  display: flex;
  align-items: center;
  /* border-bottom: 1px solid #f4f4f4; */
  padding: 30rpx;
}

.order-goods .item:last-child {
  border-bottom: none;
}

.order-goods .item .img {
  height: 145.83rpx;
  width: 145.83rpx;
  background: #f4f4f4;
}

.order-goods .item .img image {
  height: 145.83rpx;
  width: 145.83rpx;
}

.order-goods .item .info {
  flex: 1;
  margin-left: 20rpx;
  position: relative
}

.order-goods .item .t {
  margin-top: 8rpx;
  line-height: 33rpx;
  margin-bottom: 10.5rpx;
}

.order-goods .item .t .name {
  display: block;
  width: 488rpx;
  line-height: 33rpx;
  color: #333;
  font-size: 28rpx;
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.goods .item .info .flex .number {
  font-size: 28rpx;
  margin-right: 26rpx;
  margin-top: 85rpx;

}
.order-goods .item .t .number {
  display: block;
  float: right;
  /* position: absolute;
  top:88rpx;
  right: 26rpx; */
  height: 33rpx;
  text-align: right;
  line-height: 33rpx;
  color: #333;
  font-size: 28rpx;
}

.order-goods .item .attr {
  line-height: 29rpx;
  color: #666;
  /* margin-bottom: 25rpx; */
  font-size: 25rpx;
}
.order-goods .item .attr .lable{
  height: 34rpx;
  line-height: 34rpx;
  border: 1px solid #DDDEE1;
  color: #495060;
  font-size: 20rpx;
  padding:0 12rpx;
  border-radius: 3px;
  margin-right: 10rpx;
  display: block;
  margin-bottom: 6rpx
}
.order-goods .item .price {
  
  display: block;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 28rpx;
}

.priceNum {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.order-goods .item .btn {
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 5rpx;
  text-align: center;
  display: block;
  float: right;
  margin: 0 15rpx;
  padding: 0 20rpx;
}

.order-goods .item .btn.active {
  background: #E71F19;
  color: #fff;
}

.order-goods .shopPrice {
  width: 100%;
  height: 100rpx;
  padding: 10rpx 26rpx;
  position: relative
}

.order-goods .shopPrice .btn {
  height: 66rpx;
  width: 152rpx;
  line-height: 66rpx;
  border-radius: 5rpx;
  text-align: center;
  display: block;
  position: absolute;
  bottom: 15rpx;
  right: 33rpx;
  /* float: right; */
  margin: 0 15rpx;
  padding: 0 20rpx;
}

.order-goods .shopPrice .btn.active {
  background: #E71F19;
  color: #fff;
}

.order-goods .shopPrice .shopTotal {
  height: 100%;
  line-height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between
}

.order-goods .shopPrice .shopTotal .shopTotal_Title{
  font-size: 30rpx;
  font-weight: 600;
  color: #000
}

.order-goods .shopPrice .shopTotal .shopTotal_right{
  font-size: 30rpx;
  font-weight: 600;
  color: #495060
}

/* .order-goods .shopPrice .btn.active {
  background: #E71F19;
  color: #fff;
} */

.order-bottom {
  margin-top: 20rpx;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding-left: 30rpx;
}

 .address {

  padding: 30rpx;
  margin-top: 20rpx;
  border-bottom: 1px solid #f4f4f4;
  background: #fff;
}

.address .t {
  height: 35rpx;
  line-height: 35rpx;
  margin-bottom: 7.5rpx;
}

.address .name {
  display: inline-block;
  height: 35rpx;
  width: 140rpx;
  line-height: 35rpx;
  font-size: 28rpx;
}

.address .mobile {
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 28rpx;
}

.address .b {
  height: 35rpx;
  line-height: 35rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #999;
  padding-left: 30rpx
}
.address .icon{
  width:24rpx;
  height: 24rpx;
  margin-right: 20rpx;
}

.cardInfo {
  margin-top: 20rpx;
  padding: 30rpx;
  border-bottom: 1px solid #f4f4f4;
  background: #fff;
}

.order-bottom .total {
  height: 130rpx;
  padding-top: 20rpx;
  border-bottom: 1px solid #f4f4f4;
}

.order-bottom .total .t {
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 7.5rpx;
  display: flex;
}

.order-bottom .total .label {
  width: 150rpx;
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .total .txt {
  flex: 1;
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .pay-fee {
  height: 81rpx;
  line-height: 81rpx;
}

.order-bottom .pay-fee .label {
  display: inline-block;
  width: 140rpx;
  color: #b4282d;
}

.order-bottom .pay-fee .txt {
  display: inline-block;
  width: 140rpx;
  color: #b4282d;
}

.order-express {
  margin-top: 20rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
}

.order-express .title {
  float: left;
  margin-bottom: 20rpx;
  padding: 10rpx;
}

.order-express .ti {
  float: right;
  width: 52rpx;
  height: 52rpx;
  margin-right: 16rpx;
  margin-top: 28rpx;
}

.order-express .t {
  font-size: 29rpx;
  margin-left: 10.25rpx;
}

.order-express .b {
  font-size: 29rpx;
  margin-left: 10.25rpx;
}

.order-express .traces {
  padding: 17.5rpx;
  background: #fff;
  border-bottom: 1rpx solid #f1e6cdcc;
}

.order-express .trace {
  padding-bottom: 17.5rpx;
  padding-top: 17.5rpx;
  background: #fff;
}

.order-express .acceptTime {
  margin-top: 20rpx;
  margin-right: 40rpx;
  text-align: right;
  font-size: 26rpx;
}

.order-express .acceptStation {
  font-size: 26rpx;
}
